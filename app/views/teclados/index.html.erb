<p id="notice"><%= notice %></p>

<h1>Listing Teclados</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Quantidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teclados.each do |teclado| %>
      <tr>
        <td><%= teclado.tipo %></td>
        <td><%= teclado.quantidade %></td>
        <td><%= link_to 'Show', teclado %></td>
        <td><%= link_to 'Edit', edit_teclado_path(teclado) %></td>
        <td><%= link_to 'Destroy', teclado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teclado', new_teclado_path %>
