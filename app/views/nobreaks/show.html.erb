<%- model_class = Nobreak -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:capacidade) %>:</strong></dt>
  <dd><%= @nobreak.capacidade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:marca) %>:</strong></dt>
  <dd><%= @nobreak.marca %></dd>
  <dt><strong><%= model_class.human_attribute_name(:modelo) %>:</strong></dt>
  <dd><%= @nobreak.modelo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bateria) %>:</strong></dt>
  <dd><%= @nobreak.bateria %></dd>
  <dt><strong><%= model_class.human_attribute_name(:capacidade_bateria) %>:</strong></dt>
  <dd><%= @nobreak.capacidadeBateria %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              nobreaks_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_nobreak_path(@nobreak), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              nobreak_path(@nobreak),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
